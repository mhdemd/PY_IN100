    "# Display the solution\n",
    "plt.plot(t, solution[:, 0], label='Displacement (x)')\n",
    "plt.xlabel('Time (t)')\n",
    "plt.ylabel('Displacement (x)')\n",
    "plt.legend()\n",
    "plt.show()"
