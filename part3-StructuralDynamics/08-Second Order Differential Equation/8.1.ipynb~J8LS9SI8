    "from scipy.integrate import odeint\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "9f80c7b2-f903-4ec4-b635-20165e198b1a",
   "metadata": {},
   "outputs": [],
   "source": [
    "def ode_func(x, t, n):\n",
    "    dxdt = n - x\n",
    "    return dxdt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 34,
   "id": "f75e3c20-f9bb-4e54-ae3a-dd029e315b77",
   "metadata": {},
   "outputs": [],
   "source": [
    "# initial condition\n",
    "x0 = 0.5\n",
    "# Time\n",
    "t = np.linspace(0, 5, 100)\n",
    "# Parameter n\n",
    "n = 2"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 35,
   "id": "f23da971-df7b-4869-9916-d32034bde3fd",
   "metadata": {},
   "outputs": [],
   "source": [
    "# Solve\n",
    "solution = odeint(ode_func, x0, t, args=(n, ))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 36,
   "id": "ac17053a-0d9a-4297-92e1-dd0361046f8a",
